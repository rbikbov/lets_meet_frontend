{
  "name": "base_app_frontend",
  "version": "0.0.0",
  "scripts": {
    "generate:api": "sta -p ./docs/swagger.yaml -o ./src/services -n api.ts --axios --add-readonly --route-types --extract-request-params --extract-request-body --extract-response-body --extract-response-error --extract-enums",
    "generate:api:simple": "sta -p ./docs/swagger.yaml -o ./src/services -n api.ts --axios",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.0",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "swagger-typescript-api": "^12.0.2",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-pwa": "^0.13.3",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  }
}
